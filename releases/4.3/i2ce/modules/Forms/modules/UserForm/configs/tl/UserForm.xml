<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="UserForm" path="/">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="UserForm" path="/" locale="tl">
    <version>4.3.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="tl">
      <configuration name="username" path="user/username/unique" values="single" type="string">
        <value>Ang bansag na iyan ay nagamit na</value>
      </configuration>
      <configuration name="pass_match" path="user/password/mismatch" values="single" type="string">
        <value>Hindi magkapareho ang hudyat at ang hudyat na pangtiyak.</value>
      </configuration>
      <configuration name="pass_match" path="user/password/required" values="single" type="string"></configuration>
      <configuration name="role_notallowed" path="user/role/notallowed" values="single" type="string"></configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="tl">
      <configuration name="users_can_edit" locale="tl" values="single" type="string">
        <value>Makalilikha ng isang tagagamit sa sistema</value>
      </configuration>
      <configuration name="users_can_edit_all" locale="tl" values="single" type="string">
        <value>Mababago ang lahat ng mga tagagamit na nasa sistema.</value>
      </configuration>
      <configuration name="can_create_or_edit_users" locale="tl" values="single" type="string">
        <value>Makalilikha o mababago ang mga tagagamit sa loob ng sistema.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="pages" path="/I2CE/page">
      <configurationGroup name="user">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="tl" type="string">
            <value>Pamahalaan ang mga Tagagamit/Pagpunta</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="view_user">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="tl" type="string">
            <value>Pamahalaan ang mga Tagagamit/Pagpunta</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="roleForm" path="/modules/forms/forms/role">
        <configuration name="display" values="single" locale="tl" type="string">
          <value>Gampanin</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="formClasses" path="/modules/forms/formClasses">
        <configurationGroup name="I2CE_Role">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="tl" values="single">
                <value>default:Gampanin</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="assignable">
              <configuration name="headers" type="delimited" locale="tl" values="single">
                <value>default:Maitatalaga Sa Tagagamit</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="homepage">
              <configuration name="headers" type="delimited" locale="tl" values="single"></configuration>
            </configurationGroup>
            <configurationGroup name="trickle_up">
              <configuration name="headers" type="delimited" locale="tl" values="single">
                <value>default:Patuluin na Paakyat</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="I2CE_User_Form">
          <configurationGroup name="fields">
            <configurationGroup name="username">
              <configuration name="headers" type="delimited" locale="tl" values="single">
                <value>default:Pangalan ng Tagagamit</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="password">
              <configuration name="headers" type="delimited" values="many" locale="tl">
                <value>default:Hudyat (iwanang walang laman upang mapanatili ang katulad na hudyat)</value>
                <value>generate:Gumawa ng Bagong Hudyat</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="role">
              <configuration name="headers" type="delimited" locale="tl" values="single">
                <value>default:Gampanin</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>