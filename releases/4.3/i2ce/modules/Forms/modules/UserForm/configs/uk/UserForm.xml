<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="UserForm" path="/">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="UserForm" path="/" locale="uk">
    <version>4.3.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="uk">
      <configuration name="username" path="user/username/unique" values="single" type="string">
        <value>Це ім'я користувача вже використовується.</value>
      </configuration>
      <configuration name="pass_match" path="user/password/mismatch" values="single" type="string">
        <value>Пароль і підтвердження паролю не збіглися.</value>
      </configuration>
      <configuration name="pass_match" path="user/password/required" values="single" type="string">
        <value>Для нових користувачів вам потрібно ввести початковий пароль.</value>
      </configuration>
      <configuration name="role_notallowed" path="user/role/notallowed" values="single" type="string"></configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="uk">
      <configuration name="users_can_edit" locale="uk" values="single" type="string">
        <value>Можна створити користувача в системі</value>
      </configuration>
      <configuration name="users_can_edit_all" locale="uk" values="single" type="string">
        <value>Можна редагувати всіх користувачів у системі.</value>
      </configuration>
      <configuration name="can_create_or_edit_users" locale="uk" values="single" type="string">
        <value>Можна створювати або редагувати користувачів в системі.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="pages" path="/I2CE/page">
      <configurationGroup name="user">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="uk" type="string">
            <value>Керування користувачами/доступом</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="view_user">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="uk" type="string">
            <value>Керування користувачами/доступом</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="roleForm" path="/modules/forms/forms/role">
        <configuration name="display" values="single" locale="uk" type="string">
          <value>Роль</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="formClasses" path="/modules/forms/formClasses">
        <configurationGroup name="I2CE_Role">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="uk" values="single">
                <value>default:Роль</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="assignable">
              <configuration name="headers" type="delimited" locale="uk" values="single">
                <value>default:Можна призначити користувачу</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="homepage">
              <configuration name="headers" type="delimited" locale="uk" values="single">
                <value>default:Бажана Домашня сторінка</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="trickle_up">
              <configuration name="headers" type="delimited" locale="uk" values="single">
                <value>default:Вгору</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="I2CE_User_Form">
          <configurationGroup name="fields">
            <configurationGroup name="username">
              <configuration name="headers" type="delimited" locale="uk" values="single">
                <value>default:Ім'я користувача</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="password">
              <configuration name="headers" type="delimited" values="many" locale="uk">
                <value>default:Пароль (залиште порожнім, щоб зберегти той самий пароль)</value>
                <value>generate:Генерувати новий пароль</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="role">
              <configuration name="headers" type="delimited" locale="uk" values="single">
                <value>default:Роль</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>