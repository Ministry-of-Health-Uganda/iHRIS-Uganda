<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="UserForm" path="/">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="UserForm" path="/" locale="pt">
    <version>4.3.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="pt">
      <configuration name="username" path="user/username/unique" values="single" type="string"></configuration>
      <configuration name="pass_match" path="user/password/mismatch" values="single" type="string"></configuration>
      <configuration name="pass_match" path="user/password/required" values="single" type="string"></configuration>
      <configuration name="role_notallowed" path="user/role/notallowed" values="single" type="string"></configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="pt">
      <configuration name="users_can_edit" locale="pt" values="single" type="string">
        <value>Pode criar um utilizador no sistema</value>
      </configuration>
      <configuration name="users_can_edit_all" locale="pt" values="single" type="string">
        <value>Pode editar todos os utilizadores no sistema.</value>
      </configuration>
      <configuration name="can_create_or_edit_users" locale="pt" values="single" type="string">
        <value>Pode criar ou editar utilizadores no sistema.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="pages" path="/I2CE/page">
      <configurationGroup name="user">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="pt" type="string">
            <value>Gerir Utilizadores/Acesso</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="view_user">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="pt" type="string">
            <value>Gerir Utilizadores/Acesso</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="roleForm" path="/modules/forms/forms/role">
        <configuration name="display" values="single" locale="pt" type="string">
          <value>Responsabilidade</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="formClasses" path="/modules/forms/formClasses">
        <configurationGroup name="I2CE_Role">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="pt" values="single">
                <value>default:Responsabilidade</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="assignable">
              <configuration name="headers" type="delimited" locale="pt" values="single">
                <value>default:Pode Atribuir a Utilizador</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="homepage">
              <configuration name="headers" type="delimited" locale="pt" values="single"></configuration>
            </configurationGroup>
            <configurationGroup name="trickle_up">
              <configuration name="headers" type="delimited" locale="pt" values="single"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="I2CE_User_Form">
          <configurationGroup name="fields">
            <configurationGroup name="username">
              <configuration name="headers" type="delimited" locale="pt" values="single">
                <value>default:Nome de Utilizador</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="password">
              <configuration name="headers" type="delimited" values="many" locale="pt">
                <value>default:Senha (deixe em branco para manter a mesma senha)</value>
                <value>generate:Gerar Nova Senha</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="role">
              <configuration name="headers" type="delimited" locale="pt" values="single">
                <value>default:Responsabilidade</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>