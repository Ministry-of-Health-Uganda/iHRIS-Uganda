<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PrintedForms">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="PrintedForms" locale="it">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="it">
      <configuration name="printed_forms_can_access" values="single" type="string">
        <value>Consente l'accesso minimo al sistema dei moduli stampati</value>
      </configuration>
      <configuration name="printed_forms_admin" values="single" type="string">
        <value>Consente l'accesso completo al sistema dei moduli stampati</value>
      </configuration>
      <configuration name="printed_forms_all_generate" values="single" type="string">
        <value>Consente l'accesso alla generazione di tutto il sistema dei moduli stampati</value>
      </configuration>
      <configuration name="printed_forms_archive_admin" values="single" type="string">
        <value>Consente l'amministrazione di tutti gli archivi del sistema dei moduli stampati</value>
      </configuration>
      <configuration name="printed_forms_create_all_archives" values="single" type="string">
        <value>Permette la creazione di tutti gli archivi del sistema dei moduli stampati</value>
      </configuration>
      <configuration name="printed_forms_view_all_archives" values="single" type="string">
        <value>Consente la visualizzazione di tutti gli archivi del sistema dei moduli stampati</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="generated_doc">
          <configuration name="display" values="single" locale="it" type="string">
            <value>Genera Documento</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="I2CE_GeneratedDoc">
          <configurationGroup name="fields">
            <configurationGroup name="document">
              <configuration name="headers" type="delimited" locale="it" values="single">
                <value>default:Documento</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="it" values="single">
                <value>default:Data</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" values="many" locale="it">
                <value>default:Descrizione</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="primary_form">
              <configuration name="headers" type="delimited" values="many" locale="it">
                <value>default:ID del modulo primario</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>