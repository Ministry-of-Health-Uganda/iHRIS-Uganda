<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PrintedForms">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="PrintedForms" locale="de">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="de">
      <configuration name="printed_forms_can_access" values="single" type="string">
        <value>Ermöglicht minimalen Zugriff auf das System der Datenblattvordrucke</value>
      </configuration>
      <configuration name="printed_forms_admin" values="single" type="string">
        <value>Ermöglicht vollen Zugriff auf das System der Datenblattvordrucke</value>
      </configuration>
      <configuration name="printed_forms_all_generate" values="single" type="string">
        <value>Ermöglicht zum Generieren den Zugriff auf das System der Datenblattvordruck</value>
      </configuration>
      <configuration name="printed_forms_archive_admin" values="single" type="string">
        <value>Ermöglicht die Verwaltung des Systems aller archivierten Datenblattvordrucke</value>
      </configuration>
      <configuration name="printed_forms_create_all_archives" values="single" type="string">
        <value>Ermöglicht die Einrichtung des Systems aller archivierten Datenblattvordrucke</value>
      </configuration>
      <configuration name="printed_forms_view_all_archives" values="single" type="string">
        <value>Ermöglicht die Ansicht des Systems aller archivierten Datenblattvordrucke</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="generated_doc">
          <configuration name="display" values="single" locale="de" type="string">
            <value>Generiertes Dokument</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="I2CE_GeneratedDoc">
          <configurationGroup name="fields">
            <configurationGroup name="document">
              <configuration name="headers" type="delimited" locale="de" values="single">
                <value>default:Dokument</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="de" values="single">
                <value>default:Datum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" values="many" locale="de">
                <value>default:Beschreibung</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="primary_form">
              <configuration name="headers" type="delimited" values="many" locale="de">
                <value>default:Primäre Datenblattkennung</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>