<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PrintedForms">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="PrintedForms" locale="fr">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="fr">
      <configuration name="printed_forms_can_access" values="single" type="string">
        <value>Permettre d'accès minimal au système de Formulaires imprimés</value>
      </configuration>
      <configuration name="printed_forms_admin" values="single" type="string">
        <value>Permettre tout d'accès au système de Formulaires imprimés</value>
      </configuration>
      <configuration name="printed_forms_all_generate" values="single" type="string">
        <value>Permettre d'accès à générer tout du système de Formulaires imprimés</value>
      </configuration>
      <configuration name="printed_forms_archive_admin" values="single" type="string">
        <value>Permettre d'administrer tout d'archivé de Formulaires imprimés</value>
      </configuration>
      <configuration name="printed_forms_create_all_archives" values="single" type="string">
        <value>Permettre de créer tout d'archivé de Formulaires imprimés</value>
      </configuration>
      <configuration name="printed_forms_view_all_archives" values="single" type="string">
        <value>Permettre de voir tout d'archivé de Formulaires imprimés</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="generated_doc">
          <configuration name="display" values="single" locale="fr" type="string">
            <value>Générer le document</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="I2CE_GeneratedDoc">
          <configurationGroup name="fields">
            <configurationGroup name="document">
              <configuration name="headers" type="delimited" locale="fr" values="single"></configuration>
            </configurationGroup>
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="fr" values="single"></configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" values="many" locale="fr"></configuration>
            </configurationGroup>
            <configurationGroup name="primary_form">
              <configuration name="headers" type="delimited" values="many" locale="fr">
                <value>default:ID de premier formulaire</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>