<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PrintedForms">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="PrintedForms" locale="tl">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="tl">
      <configuration name="printed_forms_can_access" values="single" type="string">
        <value>Nagpapahintulot ng kaunting pagpunta sa Sistema ng Nilimbag na mga Pormularyo</value>
      </configuration>
      <configuration name="printed_forms_admin" values="single" type="string">
        <value>Nagpapahintulot ng buong pagpunta sa Sistema ng Nilimbag na mga Pormularyo</value>
      </configuration>
      <configuration name="printed_forms_all_generate" values="single" type="string">
        <value>Nagpapahintulot ng pagpunta sa paglikha ng lahat ng Sistema ng Nilimbag na mga Pormularyo</value>
      </configuration>
      <configuration name="printed_forms_archive_admin" values="single" type="string">
        <value>Nagpapahintulot ng pangangasiwa ng lahat ng nasinop na Sistema ng Nilimbag na mga Pormularyo</value>
      </configuration>
      <configuration name="printed_forms_create_all_archives" values="single" type="string">
        <value>Nagpapahintulot ng paglikha ng lahat ng nasinop na Sistema ng Nalimbag na mga Pormularyo</value>
      </configuration>
      <configuration name="printed_forms_view_all_archives" values="single" type="string">
        <value>Nagpapahintulot ng pagtanaw sa lahat ng nasinop na Sistema ng Nalimbag na mga Pormularyo</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="generated_doc">
          <configuration name="display" values="single" locale="tl" type="string">
            <value>Nalikhang Kasulatan</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="I2CE_GeneratedDoc">
          <configurationGroup name="fields">
            <configurationGroup name="document">
              <configuration name="headers" type="delimited" locale="tl" values="single">
                <value>default:Kasulatan</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="tl" values="single">
                <value>default:Petsa</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" values="many" locale="tl">
                <value>default:Paglalarawan</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="primary_form">
              <configuration name="headers" type="delimited" values="many" locale="tl">
                <value>default:ID ng Pangunahing Pormularyo</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>