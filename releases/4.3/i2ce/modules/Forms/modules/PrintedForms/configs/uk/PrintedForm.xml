<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PrintedForms">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="PrintedForms" locale="uk">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="uk">
      <configuration name="printed_forms_can_access" values="single" type="string">
        <value>Дозволяє мінімальний доступ до системи друкованих форм</value>
      </configuration>
      <configuration name="printed_forms_admin" values="single" type="string">
        <value>Дозволяє отримати повний доступ до системи друкованих форм</value>
      </configuration>
      <configuration name="printed_forms_all_generate" values="single" type="string">
        <value>Дозволяє отримати доступ до системи генерації всіх друкованих форм</value>
      </configuration>
      <configuration name="printed_forms_archive_admin" values="single" type="string">
        <value>Дозволяє управління системою усіх заархівованих друкованих форм</value>
      </configuration>
      <configuration name="printed_forms_create_all_archives" values="single" type="string">
        <value>Дозволяє створення системи усіх зархівованих друкованих форм</value>
      </configuration>
      <configuration name="printed_forms_view_all_archives" values="single" type="string">
        <value>Дозволяє перегляд системи усіх зархівованих друкованих форм</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="generated_doc">
          <configuration name="display" values="single" locale="uk" type="string">
            <value>Генерований документ</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="I2CE_GeneratedDoc">
          <configurationGroup name="fields">
            <configurationGroup name="document">
              <configuration name="headers" type="delimited" locale="uk" values="single">
                <value>default:Документ</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="uk" values="single">
                <value>default:Дата</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" values="many" locale="uk">
                <value>default:Опис</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="primary_form">
              <configuration name="headers" type="delimited" values="many" locale="uk">
                <value>default:Ідентифікатор первинної форми</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>