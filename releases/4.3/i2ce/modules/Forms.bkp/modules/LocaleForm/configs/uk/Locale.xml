<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="LocaleForm" path="/">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="LocaleForm" path="/" locale="uk">
    <version>4.3.0.1</version>
    <configurationGroup name="form" path="/modules/forms/forms/locale">
      <configuration name="display" values="single" locale="uk" type="string">
        <value>Мова</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="class" path="/modules/forms/formClasses/I2CE_Form_Locale">
      <configurationGroup name="fields">
        <configurationGroup name="name">
          <configuration name="headers" type="delimited" locale="uk" values="single">
            <value>default:Мова</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="selectable">
          <configuration name="headers" type="delimited" locale="uk" values="single">
            <value>default:На вибір</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>