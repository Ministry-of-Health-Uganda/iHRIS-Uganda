<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PrintedForms">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="PrintedForms" locale="nl">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="nl">
      <configuration name="printed_forms_can_access" values="single" type="string">
        <value>Minimale toegang geven tot het Afgedrukte formulierensysteem.</value>
      </configuration>
      <configuration name="printed_forms_admin" values="single" type="string">
        <value>Volledige toegang geven tot het Afgedrukte formulierensysteem.</value>
      </configuration>
      <configuration name="printed_forms_all_generate" values="single" type="string">
        <value>Biedt toegang tot het systeem voor het aanmaken van alle gedrukte formulieren</value>
      </configuration>
      <configuration name="printed_forms_archive_admin" values="single" type="string">
        <value>Maakt beheer mogelijk van het archief van alle gedrukte formulieren</value>
      </configuration>
      <configuration name="printed_forms_create_all_archives" values="single" type="string">
        <value>Maakt het aanmaken mogelijk van archieven voor gedrukte formulieren</value>
      </configuration>
      <configuration name="printed_forms_view_all_archives" values="single" type="string">
        <value>Maakt het bekijken mogelijk van archieven voor gedrukte formulieren</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="generated_doc">
          <configuration name="display" values="single" locale="nl" type="string">
            <value>Gegenereerd document</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="I2CE_GeneratedDoc">
          <configurationGroup name="fields">
            <configurationGroup name="document">
              <configuration name="headers" type="delimited" locale="nl" values="single">
                <value>default:Document:</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="nl" values="single">
                <value>default:Datum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" values="many" locale="nl">
                <value>default:Beschrijving</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="primary_form">
              <configuration name="headers" type="delimited" values="many" locale="nl">
                <value>default:Primair formulier-ID</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>