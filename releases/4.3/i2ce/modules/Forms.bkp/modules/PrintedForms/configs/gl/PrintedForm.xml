<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PrintedForms">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="PrintedForms" locale="gl">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="gl">
      <configuration name="printed_forms_can_access" values="single" type="string">
        <value>Permite un acceso mínimo ao sistema de formularios impresos</value>
      </configuration>
      <configuration name="printed_forms_admin" values="single" type="string">
        <value>Permite un acceso completo ao sistema de formularios impresos</value>
      </configuration>
      <configuration name="printed_forms_all_generate" values="single" type="string">
        <value>Permite o acceso para xerar todo o sistema de formularios impresos</value>
      </configuration>
      <configuration name="printed_forms_archive_admin" values="single" type="string">
        <value>Permite a administración de todos os arquivos do sistema de formularios impresos</value>
      </configuration>
      <configuration name="printed_forms_create_all_archives" values="single" type="string">
        <value>Permite a creación de todos os arquivos do sistema de formularios impresos</value>
      </configuration>
      <configuration name="printed_forms_view_all_archives" values="single" type="string">
        <value>Permite a visualización de todos os arquivos do sistema de formularios impresos</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="generated_doc">
          <configuration name="display" values="single" locale="gl" type="string">
            <value>Documento xerado</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="I2CE_GeneratedDoc">
          <configurationGroup name="fields">
            <configurationGroup name="document">
              <configuration name="headers" type="delimited" locale="gl" values="single">
                <value>default:Documento</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="gl" values="single">
                <value>default:Data</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" values="many" locale="gl">
                <value>default:Descrición</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="primary_form">
              <configuration name="headers" type="delimited" values="many" locale="gl">
                <value>default:Identificador do formulario principal</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>