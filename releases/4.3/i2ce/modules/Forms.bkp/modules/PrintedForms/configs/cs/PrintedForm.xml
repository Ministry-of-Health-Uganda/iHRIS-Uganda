<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PrintedForms">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="PrintedForms" locale="cs">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="cs">
      <configuration name="printed_forms_can_access" values="single" type="string">
        <value>Umožňuje minimální přístup do Systému Tištěných Formulářů</value>
      </configuration>
      <configuration name="printed_forms_admin" values="single" type="string">
        <value>Umožňuje úplný přístup do Systému Tištěných Formulářů</value>
      </configuration>
      <configuration name="printed_forms_all_generate" values="single" type="string">
        <value>Umožňuje přístup k vytváření všech tištěných formulářů ze Systému</value>
      </configuration>
      <configuration name="printed_forms_archive_admin" values="single" type="string">
        <value>Umožňuje správu všech archivovaných ze Systému tištěných formulářů</value>
      </configuration>
      <configuration name="printed_forms_create_all_archives" values="single" type="string">
        <value>Umožňuje vytvářet všechny archivované ze Systému tištěných formulářů</value>
      </configuration>
      <configuration name="printed_forms_view_all_archives" values="single" type="string">
        <value>Umožňuje zobrazit všechny archivované ze Systému tištěných formulářů</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="generated_doc">
          <configuration name="display" values="single" locale="cs" type="string">
            <value>Vytvořený Dokument</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="I2CE_GeneratedDoc">
          <configurationGroup name="fields">
            <configurationGroup name="document">
              <configuration name="headers" type="delimited" locale="cs" values="single">
                <value>default:Dokument</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="cs" values="single">
                <value>default:Datum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" values="many" locale="cs">
                <value>default:Popis</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="primary_form">
              <configuration name="headers" type="delimited" values="many" locale="cs">
                <value>default:ID Hlavního Formuláře</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>