<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="DatePicker" path="/modules/DatePicker">
  <metadata>
    <version>1.17.5.1</version>
  </metadata>
  <configurationGroup name="DatePicker" path="/modules/DatePicker" locale="fr">
    <version>1.17.5.1</version>
    <configurationGroup name="options_datetime">
      <configuration name="format" locale="fr"></configuration>
    </configurationGroup>
    <configurationGroup name="options">
      <configuration name="format" locale="fr"></configuration>
      <configuration name="days" type="delimited" locale="fr">
        <value>0:dimanche</value>
        <value>1:lundi</value>
        <value>2:mardi</value>
        <value>3:mercredi</value>
        <value>4:jeudi</value>
        <value>5:vendredi</value>
        <value>6:samedi</value>
      </configuration>
      <configuration name="months" type="delimited" locale="fr">
        <value>0:janvier</value>
        <value>1:février</value>
        <value>2:mars</value>
        <value>3:avril</value>
        <value>4:mai</value>
        <value>5:juin</value>
        <value>6:juillet</value>
        <value>7:août</value>
        <value>8:septembre</value>
        <value>9:octobre</value>
        <value>10:novembre</value>
        <value>11:décembre</value>
      </configuration>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>