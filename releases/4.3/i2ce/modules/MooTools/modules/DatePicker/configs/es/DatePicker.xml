<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="DatePicker" path="/modules/DatePicker">
  <metadata>
    <version>1.17.5.1</version>
  </metadata>
  <configurationGroup name="DatePicker" path="/modules/DatePicker" locale="es">
    <version>1.17.5.1</version>
    <configurationGroup name="options_datetime">
      <configuration name="format" locale="es"></configuration>
    </configurationGroup>
    <configurationGroup name="options">
      <configuration name="format" locale="es"></configuration>
      <configuration name="days" type="delimited" locale="es">
        <value>0:Domingo</value>
        <value>1:Lunes</value>
        <value>2:Martes</value>
        <value>3:Miércoles</value>
        <value>4:Jueves</value>
        <value>5:Viernes</value>
        <value>6:Sábado</value>
      </configuration>
      <configuration name="months" type="delimited" locale="es">
        <value>0:Enero</value>
        <value>1:Febrero</value>
        <value>2:Marzo</value>
        <value>3:Abril</value>
        <value>4:Mayo</value>
        <value>5:Junio</value>
        <value>6:Julio</value>
        <value>7:Agosto</value>
        <value>8:Septiembre</value>
        <value>9:Octubre</value>
        <value>10:Noviembre</value>
        <value>11:Diciembre</value>
      </configuration>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>