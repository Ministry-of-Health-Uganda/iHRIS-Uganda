<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="CustomReports_PieChart" path="/modules/CustomReports/displays/PieChart">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="CustomReports_PieChart" path="/modules/CustomReports/displays/PieChart" locale="nl">
    <version>4.3.0.1</version>
    <configuration name="error_message" locale="nl" values="single" type="string">
      <value>Er zijn geen gegevens gevonden voor deze rapportage. Zorg ervoor dat de rapportgegevens in de cache zitten of wijzig uw limieten.</value>
    </configuration>
    <configurationGroup name="display_options">
      <configurationGroup name="styles">
        <configurationGroup name="Pie">
          <configuration name="display_name" locale="nl" values="single" type="string">
            <value>Cirkeldiagram (eenvoudig)</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="Bar">
          <configuration name="display_name" locale="nl" values="single" type="string">
            <value>Staafdiagram (eenvoudig)</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="Column">
          <configuration name="display_name" locale="nl" values="single" type="string">
            <value>Kolomdiagram (eenvoudig)</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="StackedColumn">
          <configuration name="display_name" locale="nl" values="single" type="string">
            <value>Gestapeld kolomdiagram (eenvoudig)</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="Scatter">
          <configuration name="display_name" locale="nl" values="single" type="string">
            <value>Spreidingsdiagram</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>