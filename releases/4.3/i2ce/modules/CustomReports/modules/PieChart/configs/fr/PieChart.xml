<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="CustomReports_PieChart" path="/modules/CustomReports/displays/PieChart">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="CustomReports_PieChart" path="/modules/CustomReports/displays/PieChart" locale="fr">
    <version>4.3.0.1</version>
    <configuration name="error_message" locale="fr" values="single" type="string">
      <value>Pas de données pour ce rapport. Vérifiez que des données existent ou modifiez votre champ de recherche.</value>
    </configuration>
    <configurationGroup name="display_options">
      <configurationGroup name="styles">
        <configurationGroup name="Pie">
          <configuration name="display_name" locale="fr" values="single" type="string">
            <value>Camembert (Simple)</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="Bar">
          <configuration name="display_name" locale="fr" values="single" type="string">
            <value>Diagramme à barres horizontales (simple)</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="Column">
          <configuration name="display_name" locale="fr" values="single" type="string">
            <value>Diagramme à colonnes (Simple)</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="StackedColumn">
          <configuration name="display_name" locale="fr" values="single" type="string">
            <value>Diagramme à colonnes empilées (simple)</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="Scatter">
          <configuration name="display_name" locale="fr" values="single" type="string">
            <value>Diagramme de dispersion</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>