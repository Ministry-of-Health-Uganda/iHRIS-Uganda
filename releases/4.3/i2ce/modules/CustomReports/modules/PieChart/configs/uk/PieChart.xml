<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="CustomReports_PieChart" path="/modules/CustomReports/displays/PieChart">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="CustomReports_PieChart" path="/modules/CustomReports/displays/PieChart" locale="uk">
    <version>4.3.0.1</version>
    <configuration name="error_message" locale="uk" values="single" type="string">
      <value>Немає даних для цього звіту. Будь ласка, переконайтеся, що у звіті є дані з кешу або змініть свої межі.</value>
    </configuration>
    <configurationGroup name="display_options">
      <configurationGroup name="styles">
        <configurationGroup name="Pie">
          <configuration name="display_name" locale="uk" values="single" type="string">
            <value>Кругова діаграма (проста)</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="Bar">
          <configuration name="display_name" locale="uk" values="single" type="string">
            <value>Гістограма (проста)</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="Column">
          <configuration name="display_name" locale="uk" values="single" type="string">
            <value>Стовпчикова діаграма (проста)</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="StackedColumn">
          <configuration name="display_name" locale="uk" values="single" type="string">
            <value>Гістограма з накопиченням (проста)</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="Scatter">
          <configuration name="display_name" locale="uk" values="single" type="string">
            <value>Точкова діаграма</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>