<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="CustomReports_PieChart" path="/modules/CustomReports/displays/PieChart">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="CustomReports_PieChart" path="/modules/CustomReports/displays/PieChart" locale="cs">
    <version>4.3.0.1</version>
    <configuration name="error_message" locale="cs" values="single" type="string">
      <value>Pro toto hlášení nebyla nalezena žádná data. Ujistěte, se prosím, že data hlášení byla uložena nebo změňte Vaše omezení.</value>
    </configuration>
    <configurationGroup name="display_options">
      <configurationGroup name="styles">
        <configurationGroup name="Pie">
          <configuration name="display_name" locale="cs" values="single" type="string">
            <value>Koláčový graf (jednoduchý)</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="Bar">
          <configuration name="display_name" locale="cs" values="single" type="string">
            <value>Pruhový graf (jednoduchý)</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="Column">
          <configuration name="display_name" locale="cs" values="single" type="string">
            <value>Sloupcový graf (Jednoduchý)</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="StackedColumn">
          <configuration name="display_name" locale="cs" values="single" type="string">
            <value>Skládaný sloupcový graf (Jednoduchý)</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="Scatter">
          <configuration name="display_name" locale="cs" values="single" type="string">
            <value>Bodový graf</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>