<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ReportArchiver">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ReportArchiver" locale="nl">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="nl">
      <configuration name="custom_reports_archiver_can_view" locale="nl" values="single" type="string">
        <value>Maakt het mogelijk om gearchiveerde maatwerkrapportages te bekijken</value>
      </configuration>
      <configuration name="custom_reports_archiver_can_archive" locale="nl" values="single" type="string">
        <value>Maakt het mogelijk om maatwerkrapportages te archiveren</value>
      </configuration>
      <configuration name="custom_reports_archiver_can_delete" locale="nl" values="single" type="string">
        <value>Maakt het mogelijk om gearchiveerde maatwerkrapportages te verwijderen</value>
      </configuration>
      <configuration name="custom_reports_archiver_admin" locale="nl" values="single" type="string">
        <value>Geef volledige toegang tot het archiveringssysteem voor maatwerkrapportages</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="archived_report">
          <configuration name="display" values="single" locale="nl" type="string">
            <value>Gearchiveerde rapportage</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="I2CE_ArchivedReport">
          <configurationGroup name="meta" path="meta/list/default">
            <configuration name="display_string" locale="nl" values="single" type="string">
              <value>%s aangemaakt op %s</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="fields">
            <configurationGroup name="report">
              <configuration name="headers" type="delimited" locale="nl" values="single">
                <value>default:Rapportage</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="nl" values="single">
                <value>default:Aangemaakt op</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="report_view">
              <configuration name="headers" type="delimited" values="many" locale="nl">
                <value>default:Rapportweergave</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" values="many" locale="nl">
                <value>default:Naam</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="CLI_archive_page" path="/modules/CustomReports/command_line/archive">
      <configurationGroup name="args">
        <configuration name="title" locale="nl" values="single" type="string">
          <value>Archiefrapportage</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="archive_page" path="/modules/CustomReports/page/archive">
      <configurationGroup name="args">
        <configuration name="title" locale="nl" values="single" type="string">
          <value>Archiefrapportage</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="view_archives_page" path="/modules/CustomReports/page/viewArchives">
      <configurationGroup name="args">
        <configuration name="title" locale="nl" values="single" type="string">
          <value>Gearchiveerde rapportages bekijken</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>