<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ReportArchiver">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ReportArchiver" locale="pt_BR">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="pt_BR">
      <configuration name="custom_reports_archiver_can_view" locale="pt_BR" values="single" type="string">
        <value>Permite visão dos relatórios personalizados arquivados</value>
      </configuration>
      <configuration name="custom_reports_archiver_can_archive" locale="pt_BR" values="single" type="string">
        <value>Permite arquivamento de relatórios personalizados</value>
      </configuration>
      <configuration name="custom_reports_archiver_can_delete" locale="pt_BR" values="single" type="string">
        <value>Permite exclusão de relatórios personalizados arquivados</value>
      </configuration>
      <configuration name="custom_reports_archiver_admin" locale="pt_BR" values="single" type="string">
        <value>Permite acesso total ao sistema de arquivamento de relatórios personalizados</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="archived_report">
          <configuration name="display" values="single" locale="pt_BR" type="string">
            <value>Relatório arquivado</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="I2CE_ArchivedReport">
          <configurationGroup name="meta" path="meta/list/default">
            <configuration name="display_string" locale="pt_BR" values="single" type="string">
              <value>%s gerado em %s</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="fields">
            <configurationGroup name="report">
              <configuration name="headers" type="delimited" locale="pt_BR" values="single">
                <value>default:Relatório</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="pt_BR" values="single">
                <value>default:Data de geração</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="report_view">
              <configuration name="headers" type="delimited" values="many" locale="pt_BR">
                <value>default:Visualização do relatório</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" values="many" locale="pt_BR">
                <value>default:Título</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="CLI_archive_page" path="/modules/CustomReports/command_line/archive">
      <configurationGroup name="args">
        <configuration name="title" locale="pt_BR" values="single" type="string">
          <value>Arquivar relatório</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="archive_page" path="/modules/CustomReports/page/archive">
      <configurationGroup name="args">
        <configuration name="title" locale="pt_BR" values="single" type="string">
          <value>Arquivar relatório</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="view_archives_page" path="/modules/CustomReports/page/viewArchives">
      <configurationGroup name="args">
        <configuration name="title" locale="pt_BR" values="single" type="string">
          <value>Ver relatórios arquivados</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>