<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ReportArchiver">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ReportArchiver" locale="es">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="es">
      <configuration name="custom_reports_archiver_can_view" locale="es" values="single" type="string">
        <value>Permite visualizar Reportes Personalizados Archivados</value>
      </configuration>
      <configuration name="custom_reports_archiver_can_archive" locale="es" values="single" type="string">
        <value>Permitir el Archivado de Reportes Personalizados</value>
      </configuration>
      <configuration name="custom_reports_archiver_can_delete" locale="es" values="single" type="string">
        <value>Permitir el borrado de Reportes Personalizados archivados</value>
      </configuration>
      <configuration name="custom_reports_archiver_admin" locale="es" values="single" type="string">
        <value>Permitir acceso completo al sistema de Archivado de Reportes Personalizados</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="archived_report">
          <configuration name="display" values="single" locale="es" type="string">
            <value>Reporte Archivado</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="I2CE_ArchivedReport">
          <configurationGroup name="meta" path="meta/list/default">
            <configuration name="display_string" locale="es" values="single" type="string">
              <value>%s generado el %s</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="fields">
            <configurationGroup name="report">
              <configuration name="headers" type="delimited" locale="es" values="single">
                <value>default:Informe</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="es" values="single">
                <value>default:Fecha de Generación</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="report_view">
              <configuration name="headers" type="delimited" values="many" locale="es">
                <value>default:Vista de informe</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" values="many" locale="es">
                <value>default:Título</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="CLI_archive_page" path="/modules/CustomReports/command_line/archive">
      <configurationGroup name="args">
        <configuration name="title" locale="es" values="single" type="string">
          <value>Archivar reporte</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="archive_page" path="/modules/CustomReports/page/archive">
      <configurationGroup name="args">
        <configuration name="title" locale="es" values="single" type="string">
          <value>Archivar reporte</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="view_archives_page" path="/modules/CustomReports/page/viewArchives">
      <configurationGroup name="args">
        <configuration name="title" locale="es" values="single" type="string">
          <value>Visualizar Reportes Archivados</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>