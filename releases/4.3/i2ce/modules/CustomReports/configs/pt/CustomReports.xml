<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="CustomReports">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="CustomReports" locale="pt">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/modules/CustomReports/text/" locale="pt">
      <configuration name="Increasing" values="single" type="string">
        <value>Aumentar</value>
      </configuration>
      <configuration name="Decreasing" values="single" type="string">
        <value>Diminuir</value>
      </configuration>
      <configurationGroup name="user_messages">
        <configuration name="no_report_view" values="single" type="string"></configuration>
        <configuration name="invalid_report_view" values="single" type="string"></configuration>
        <configuration name="no_permission_report_view" values="single" type="string"></configuration>
        <configuration name="no_display_report_view" values="single" type="string"></configuration>
        <configuration name="reportView_merged_field_display" values="single" type="string">
          <value>%s de %s</value>
        </configuration>
        <configuration name="merge_display" values="single" type="string">
          <value>%s vinculado em %s</value>
        </configuration>
        <configurationGroup name="merge_descriptions">
          <configuration name="parent" values="single" type="string"></configuration>
          <configuration name="child" values="single" type="string"></configuration>
          <configuration name="default" values="single" type="string">
            <value>%s vinculado em %s</value>
          </configuration>
        </configurationGroup>
        <configuration name="report_no_name" values="single" type="string"></configuration>
        <configuration name="report_bad_name" values="single" type="string"></configuration>
        <configuration name="report_bad_used" values="single" type="string"></configuration>
        <configuration name="report_no_relationship" values="single" type="string"></configuration>
        <configuration name="report_bad_relationship" values="single" type="string"></configuration>
        <configuration name="cannot_delete" values="single" type="string"></configuration>
        <configuration name="bad_delete_path" values="single" type="string"></configuration>
        <configuration name="not_cached" values="single" type="string"></configuration>
        <configuration name="not_saved" values="single" type="string"></configuration>
        <configuration name="saved" values="single" type="string"></configuration>
        <configuration name="not_generated" values="single" type="string"></configuration>
        <configuration name="not_updated" values="single" type="string"></configuration>
        <configuration name="updated" values="single" type="string"></configuration>
      </configurationGroup>
      <configurationGroup name="report_status">
        <configuration name="does_not_exist" values="single" type="string"></configuration>
        <configuration name="not_generated" values="single" type="string"></configuration>
        <configuration name="stale" values="single" type="string"></configuration>
        <configuration name="failed" values="single" type="string">
          <value>Falhado</value>
        </configuration>
        <configuration name="in_progress" values="single" type="string">
          <value>Em progresso</value>
        </configuration>
        <configuration name="generated" values="single" type="string">
          <value>Gerado</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="headers">
        <configuration name="sum" values="single" type="string">
          <value>Soma</value>
        </configuration>
        <configuration name="maximum" values="single" type="string">
          <value>Máximo</value>
        </configuration>
        <configuration name="minimum" values="single" type="string">
          <value>Mínimo</value>
        </configuration>
        <configuration name="average" values="single" type="string">
          <value>Média</value>
        </configuration>
        <configuration name="count" values="single" type="string"></configuration>
        <configuration name="count_distinct" values="single" type="string">
          <value>Total Distinto</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="pt">
      <configuration name="custom_reports_can_access" locale="pt" values="single" type="string"></configuration>
      <configuration name="custom_reports_delete" locale="pt" values="single" type="string">
        <value>Permite a eliminação de dados sobre relatórios customizados</value>
      </configuration>
      <configuration name="custom_reports_can_access_relationships" locale="pt" values="single" type="string">
        <value>Permite acesso às Relações de Reporte Customizado</value>
      </configuration>
      <configuration name="custom_reports_can_edit_relationships" locale="pt" values="single" type="string">
        <value>Permite a edição de Relações de Relatório Customizado</value>
      </configuration>
      <configuration name="custom_reports_can_access_reports" locale="pt" values="single" type="string">
        <value>Permite Acesso a Relatórios Customizados</value>
      </configuration>
      <configuration name="custom_reports_can_edit_reports" locale="pt" values="single" type="string"></configuration>
      <configuration name="custom_reports_can_view_reportViews" locale="pt" values="single" type="string">
        <value>Permite a Visualização de Relatórios Customizados</value>
      </configuration>
      <configuration name="custom_reports_can_edit_reportViews" locale="pt" values="single" type="string">
        <value>Permite a edição da Visualização de Relatórios Customizados</value>
      </configuration>
      <configuration name="custom_reports_admin" locale="pt" values="single" type="string">
        <value>Administrador de relatórios customizados. Pode realizar todas as tarefas associadas aos relatórios customizados</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="command_line">
      <configurationGroup name="show">
        <configurationGroup name="args">
          <configuration name="title" locale="pt" values="single" type="string">
            <value>Ver Relatórios</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="page_styles" path="/I2CE/template/page_styles">
        <configurationGroup name="customreport_config">
          <configurationGroup name="args">
            <configuration name="title" locale="pt" values="single" type="string">
              <value>Criar Relatórios</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="show">
        <configurationGroup name="args">
          <configuration name="title" locale="pt" values="single" type="string">
            <value>Ver Relatórios</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="saveOptions">
        <configurationGroup name="args">
          <configuration name="title" locale="pt" values="single" type="string">
            <value>Ver Relatórios</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="nodisplay">
        <configurationGroup name="args">
          <configuration name="title" locale="pt" values="single" type="string"></configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>