<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ReportArchiver">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ReportArchiver" locale="uk">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="uk">
      <configuration name="custom_reports_archiver_can_view" locale="uk" values="single" type="string">
        <value>Дозволяє перегляд заархівованих власних звітів</value>
      </configuration>
      <configuration name="custom_reports_archiver_can_archive" locale="uk" values="single" type="string">
        <value>Дозволяє архівування власних звітів</value>
      </configuration>
      <configuration name="custom_reports_archiver_can_delete" locale="uk" values="single" type="string">
        <value>Дозволяє вилучення заархівованих власних звітів</value>
      </configuration>
      <configuration name="custom_reports_archiver_admin" locale="uk" values="single" type="string">
        <value>Надає повний доступ для системи архівування власних звітів</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="archived_report">
          <configuration name="display" values="single" locale="uk" type="string">
            <value>Заархівований звіт</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="I2CE_ArchivedReport">
          <configurationGroup name="meta" path="meta/list/default">
            <configuration name="display_string" locale="uk" values="single" type="string">
              <value>%s згенеровано на %s</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="fields">
            <configurationGroup name="report">
              <configuration name="headers" type="delimited" locale="uk" values="single">
                <value>default:Звіт</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="uk" values="single">
                <value>default:Дата створення</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="report_view">
              <configuration name="headers" type="delimited" values="many" locale="uk">
                <value>default:Подання звіту</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" values="many" locale="uk">
                <value>default:Назва</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="CLI_archive_page" path="/modules/CustomReports/command_line/archive">
      <configurationGroup name="args">
        <configuration name="title" locale="uk" values="single" type="string">
          <value>Архівний звіт</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="archive_page" path="/modules/CustomReports/page/archive">
      <configurationGroup name="args">
        <configuration name="title" locale="uk" values="single" type="string">
          <value>Архівний звіт</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="view_archives_page" path="/modules/CustomReports/page/viewArchives">
      <configurationGroup name="args">
        <configuration name="title" locale="uk" values="single" type="string">
          <value>Переглянути заархівовані звіти</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>